  object QryGC: TIBOQuery
    IB_Connection = DM1.DBAzienda
    IB_Transaction = TransGC
    RecordCountAccurate = True
    OnCalcFields = QryGCCalcFields
    SQL.Strings = (
      'SELECT V.CODICEARTICOLO, A.DESCRIZIONE, A.UNITADIMISURA'
      ''
      ' ,CAST(NULL AS TIMESTAMP) AS DATADOCUMENTO'
      ''
      ' ,V.SOTTOCANTIERE1, V.SOTTOCANTIERE2, V.SOTTOCANTIERE'
      ''
      
        ' ,CAST((SELECT G.Descrizione FROM Gruppi G WHERE G.Codice1=A.Cod' +
        'iceGruppo1 AND G.Codice2=-1 AND G.Codice3=-1 AND G.Codice4=-1 AN' +
        'D G.Codice5=-1 AND G.Codice6=-1) || '#39'  ('#39' || A.CodiceGruppo1 || ' +
        #39')'#39' AS VARCHAR(70)) AS GRUPPO1'
      
        ' ,CAST((SELECT G.Descrizione FROM Gruppi G WHERE G.Codice1=A.Cod' +
        'iceGruppo1 AND G.Codice2=A.CodiceGruppo2 AND G.Codice3=-1 AND G.' +
        'Codice4=-1 AND G.Codice5=-1 AND G.Codice6=-1) || '#39'  ('#39' || A.Codi' +
        'ceGruppo2 || '#39')'#39' AS VARCHAR(70)) AS GRUPPO2'
      
        ' ,CAST((SELECT G.Descrizione FROM Gruppi G WHERE G.Codice1=A.Cod' +
        'iceGruppo1 AND G.Codice2=A.CodiceGruppo2 AND G.Codice3=A.CodiceG' +
        'ruppo3 AND G.Codice4=-1 AND G.Codice5=-1 AND G.Codice6=-1) || '#39' ' +
        ' ('#39' || A.CodiceGruppo3 || '#39')'#39' AS VARCHAR(70)) AS GRUPPO3'
      
        ' ,CAST((SELECT G.Descrizione FROM Gruppi G WHERE G.Codice1=A.Cod' +
        'iceGruppo1 AND G.Codice2=A.CodiceGruppo2 AND G.Codice3=A.CodiceG' +
        'ruppo3 AND G.Codice4=A.CodiceGruppo4 AND G.Codice5=-1 AND G.Codi' +
        'ce6=-1) || '#39'  ('#39' || A.CodiceGruppo4 || '#39')'#39
      '     AS VARCHAR(70)) AS GRUPPO4'
      
        ' ,CAST((SELECT G.Descrizione FROM Gruppi G WHERE G.Codice1=A.Cod' +
        'iceGruppo1 AND G.Codice2=A.CodiceGruppo2 AND G.Codice3=A.CodiceG' +
        'ruppo3 AND G.Codice4=A.CodiceGruppo4 AND G.Codice5=A.CodiceGrupp' +
        'o5 AND G.Codice6=-1) || '#39'  ('#39' || A.CodiceGruppo5 || '#39')'#39
      '     AS VARCHAR(70)) AS GRUPPO5'
      
        ' ,CAST((SELECT G.Descrizione FROM Gruppi G WHERE G.Codice1=A.Cod' +
        'iceGruppo1 AND G.Codice2=A.CodiceGruppo2 AND G.Codice3=A.CodiceG' +
        'ruppo3 AND G.Codice4=A.CodiceGruppo4 AND G.Codice5=A.CodiceGrupp' +
        'o5 AND G.Codice6=A.CodiceGruppo6)'
      
        '     || '#39'  ('#39' || A.CodiceGruppo6 || '#39')'#39' AS VARCHAR(70)) AS GRUPP' +
        'O6'
      ''
      ' ,SUM(V.GC_COMMESSA_QTA) AS COMMESSA_QTA'
      ' ,SUM(V.GC_COMMESSA_IMPORTOACQ) AS COMMESSA_IMPORTOACQ'
      ' ,SUM(V.GC_COMMESSA_IMPORTOVEND) AS COMMESSA_IMPORTOVEND'
      ' ,SUM(V.GC_COMMESSA_GUADAGNOIMPORTO) AS COMMESSA_GUADAGNOIMPORTO'
      ''
      ' ,SUM(V.GC_GIACENZA_QTA) AS GIACENZA_QTA'
      ' ,SUM(V.GC_GIACENZA_IMPORTOACQ) AS GIACENZA_IMPORTOACQ'
      ' ,SUM(V.GC_GIACENZA_IMPORTOVEND) AS GIACENZA_IMPORTOVEND'
      ' ,SUM(V.GC_GIACENZA_GUADAGNOIMPORTO) AS GIACENZA_GUADAGNOIMPORTO'
      ''
      ' ,SUM(V.GC_MONTATO_QTA) AS MONTATO_QTA'
      ' ,SUM(V.GC_MONTATO_IMPORTOACQ) AS MONTATO_IMPORTOACQ'
      ' ,SUM(V.GC_MONTATO_IMPORTOVEND) AS MONTATO_IMPORTOVEND'
      ' ,SUM(V.GC_MONTATO_GUADAGNOIMPORTO) AS MONTATO_GUADAGNOIMPORTO'
      ''
      ' ,SUM(V.GC_FATTURATO_IMPORTOVEND) AS FATTURATO_IMPORTOVEND'
      ''
      
        ' ,SUM(V.GC_DAMONT_GIACENZA_IMPORTOVEND) AS DAMONT_GIACENZA_IMPOR' +
        'TOVEND'
      
        ' ,SUM(V.GC_DAMONT_COMMESSA_IMPORTOVEND) AS DAMONT_COMMESSA_IMPOR' +
        'TOVEND'
      ''
      
        ' ,SUM(V.GC_DAFATT_GIACENZA_IMPORTOVEND) AS DAFATT_GIACENZA_IMPOR' +
        'TOVEND'
      
        ' ,SUM(V.GC_DAFATT_COMMESSA_IMPORTOVEND) AS DAFATT_COMMESSA_IMPOR' +
        'TOVEND'
      
        ' ,SUM(V.GC_DAFATT_MONTATO_IMPORTOVEND) AS DAFATT_MONTATO_IMPORTO' +
        'VEND'
      ''
      ' ,MA.MAG_GIACENZA'
      ' ,MA.MAG_ORDINATO'
      ' ,MA.MAG_IMPEGNATO'
      ''
      ' ,CAST(NULL AS NUMERIC(11,4)) AS MAG_ULTIMOPREZZOACQUISTO'
      ''
      ' ,CAST('#39'Materiali / Apparecchi'#39' AS VARCHAR(30)) AS TIPOLOGIA'
      ''
      ' ,V.RIFPRATICA'
      
        ' ,V.CATEGCANT1 ,V.CATEGCANT2 ,V.CATEGCANT3 ,V.CATEGCANT4 ,V.CATE' +
        'GCANT5 ,V.CATEGCANT6'
      ''
      
        ' FROM GC_BASE (NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL' +
        ',NULL,NULL,NULL) V'
      ' LEFT JOIN ARTICOLI A ON (A.CODICEARTICOLO=V.CODICEARTICOLO)'
      ' LEFT JOIN'
      
        '   (SELECT M.CODICEARTICOLO, SUM(M.GIACENZA) AS MAG_GIACENZA, SU' +
        'M(M.ORDINATO) AS MAG_ORDINATO, SUM(M.IMPEGNATO) AS MAG_IMPEGNATO' +
        ' FROM MAG_ART AS M GROUP BY M.CODICEARTICOLO)'
      '   AS MA ON (MA.CODICEARTICOLO = V.CODICEARTICOLO)'
      ''
      ' GROUP BY CODICEARTICOLO'
      '         ,DESCRIZIONE'
      '         ,UNITADIMISURA'
      '         ,MAG_ULTIMOPREZZOACQUISTO'
      '         ,TIPOLOGIA'
      '         ,V.SOTTOCANTIERE1, V.SOTTOCANTIERE2, V.SOTTOCANTIERE3'
      
        '         ,A.CodiceGruppo1, A.CodiceGruppo2, A.CodiceGruppo3, A.C' +
        'odiceGruppo4, A.CodiceGruppo5, A.CodiceGruppo6'
      '         ,MA.MAG_GIACENZA, MA.MAG_ORDINATO, MA.MAG_IMPEGNATO'
      '         ,V.RIFPRATICA'
      
        '         ,V.CATEGCANT1 ,V.CATEGCANT2 ,V.CATEGCANT3 ,V.CATEGCANT4' +
        ' ,V.CATEGCANT5 ,V.CATEGCANT6'
      ''
      '')
    Left = 182
    Top = 190
    object QryGCCODICEARTICOLO: TStringField
      FieldName = 'CODICEARTICOLO'
      Size = 25
    end
