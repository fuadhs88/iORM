    IB_Connection = DM1.DBAzienda
    IB_Transaction = TransGCAda
    RecordCountAccurate = True
    OnCalcFields = QryGCCalcFields
    SQL.Strings = (
      'SELECT R.CODICEARTICOLO, A.DESCRIZIONE, A.UNITADIMISURA,'
      
        ' R.SOTTOCANTIERE1,R.SOTTOCANTIERE2,R.SOTTOCANTIERE3,R.SOTTOCANTI' +
        'ERE4,R.SOTTOCANTIERE5,R.SOTTOCANTIERE6,'
      
        ' R.QTA, R.SEGNOOPERAZIONECANTIERE, R.IMPORTORIGO, R.IMPORTORIGOI' +
        'VACOMPRESA,'
      
        ' CAST( (SELECT SUM(M2.DISPONIBILE)  FROM MAGAZZINI M2 WHERE M2.C' +
        'ODICEARTICOLO = R.CODICEARTICOLO) AS NUMERIC (11,4)) AS QTADISPO' +
        'NIBILE'
      ''
      ''
      'FROM RIGHIPRV R'
      ' LEFT JOIN ARTICOLI A ON (A.CODICEARTICOLO=R.CODICEARTICOLO)'
      
        ' INNER JOIN PRVORDCL T ON (R.TIPODOCUMENTO=T.TIPODOCUMENTO AND R' +
        '.REGISTRO=T.REGISTRO AND R.NUMORDPREV=T.NUMORDPREV AND R.DATADOC' +
        'UMENTO=T.DATADOCUMENTO)')
    Left = 182
    Top = 222
