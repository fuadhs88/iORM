A_PRELIEVO_LISTINO_PREZZO:=FieldType:STRING;	MAXLENGTH:60;
A_DESCRIZIONE_ARTICOLO_PRELIEVO_LISTINO:=FieldType:STRING;	MAXLENGTH:60;
A_UM_QTA1=FieldType:NUMBER;
A_QTA1=FieldType:NUMBER;
A_PREZZO1_UVA=FieldType:NUMBER;
