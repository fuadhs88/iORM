[DbFieldList]
ID=Value:<A_ID_RIGA>; IsKeyField:YES;
IDPRATICA=Value:(SELECT PA.IDPRATICA FROM PRATAPPARECCHI PA WHERE PA.ID = <A_ID_RIGA_CONTRATTO>);
IDAPPARECCHIOPRAT=Value:<A_ID_RIGA_CONTRATTO>;
IDOP=Value:<A_CODICE_ARTICOLO_PRESTAZIONE>;
DESCRIZIONE=Value:<A_DESCRIZIONE_ARTICOLO>;
NOTE=Value:<A_NOTE>;
PERIODICITA=Value:(CASE WHEN(<A_DURATA_PERIODICITA> = 3) THEN 90 WHEN(<A_DURATA_PERIODICITA> = 6) THEN 180 WHEN(<A_DURATA_PERIODICITA> = 12) THEN 365 WHEN(<A_DURATA_PERIODICITA> = 22) THEN 660 WHEN(<A_DURATA_PERIODICITA> = 24) THEN 730 WHEN(<A_DURATA_PERIODICITA> = 36) THEN 1095 WHEN(<A_DURATA_PERIODICITA> = 48) THEN 1460 WHEN(<A_DURATA_PERIODICITA> = 60) THEN 1825 END);
ULTIMA=Value:<A_DATA_PREVISTA_INTERVENTO>;
PROSSIMA=Value:<A_INTERVENTO_PREVISTO>;
PRZUNIT=Value:<A_UM_PREZZO_IMPOSTATO>;
PRZUNITNETTO=Value:<A_UM_PREZZO_IMPOSTATO>;
CODICEIVA=Value:1;
PRZUNITIVACOMP=Value:<A_UM_PREZZO_IMPOSTATO> * 1.22;
PRZUNITNETTOIVACOMP=Value:<A_UM_PREZZO_IMPOSTATO> * 1.22;
ADDEBITA=Value:'T';
INABBONAMENTO=Value:'T';
GG_AUTOSEL=Value:60;

